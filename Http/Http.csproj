<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <Version>1.0.1</Version>
    <PackageId>SartainStudios.Http</PackageId>
    <Authors>John Sartain</Authors>
    <Company>Sartain Studios</Company>
    <Product>Sartain Studios Http</Product>
    <Description>Simple http library</Description>
    <Copyright>Sartain Studios</Copyright>
    <PackageProjectUrl>https://github.com/jhnsartain3/SartainStudiosHttp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/jhnsartain3/SartainStudiosHttp</RepositoryUrl>
    <RepositoryType>Nuget Package</RepositoryType>
    <PackageTags>Http</PackageTags>
    <RootNamespace>SartainStudios.Http</RootNamespace>
    <AssemblyName>SartainStudios.Http</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoWrapper.Core" Version="4.5.0" />
    <PackageReference Include="Httwrap" Version="2.0.2" />
  </ItemGroup>

</Project>
